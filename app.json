{
  "expo": {
    "name": "Instagram",
    "slug": "Instagram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoInsta.png",
    "splash": {
      "image": "./assets/telaInicialApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoInsta.png",
        "backgroundColor": "#000000"
      },
      "package": "com.ylu1gi.Instagram"
    },
    "web": {
      "favicon": "./assets/logoInsta.png"
    }
  }
}
